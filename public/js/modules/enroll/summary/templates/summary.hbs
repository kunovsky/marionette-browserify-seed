<div class="page-header">
  <h1>{{cms "summary_heading"}}</h1>
</div>

<div class="form-horizontal well" role="form">
 <div class="form-group">
    <label class="col-sm-1 control-label" for="name">{{cms "summary_name"}}</label>
    <div class="col-sm-11">
      <input class="form-control" name="name" value="{{name}}" type="text" readonly="readonly">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-1 control-label" for="age">{{cms "summary_age"}}</label>
    <div class="col-sm-11">
      <input class="form-control" name="age" value="{{age}}" type="text" readonly="readonly">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-1 control-label" for="email">{{cms "summary_email"}}</label>
    <div class="col-sm-11">
      <input class="form-control" name="email" value="{{email}}" type="text" readonly="readonly">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-1 control-label" for="phone">{{cms "sumamry_phone"}}</label>
    <div class="col-sm-11">
      <input class="form-control" name="phone" value="{{phone}}" type="text" readonly="readonly">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-1 col-sm-11">
      <a href="/#">
       <button class="complete btn btn-default">{{cms "home"}}</button>
      </a>
     </div>
   </div>
</div>
